{
  "name": "kuaukutsu/ps-onion",
  "description": "Project Structure: onion",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitriy Krivopalov",
      "email": "dimarik82@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/kuaukutsu/ps-onion/issues",
    "source": "https://github.com/kuaukutsu/ps-onion"
  },
  "require": {
    "php": ">=8.3",
    "ramsey/uuid": "^4.7",
    "php-di/php-di": "^7.0.5",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^2.0",
    "guzzlehttp/guzzle": "^7.1"
  },
  "require-dev": {
    "vimeo/psalm": "^5.25",
    "roave/infection-static-analysis-plugin": "^1.3",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "kuaukutsu\\ps\\onion\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kuaukutsu\\ps\\onion\\tests\\": "tests"
    }
  }
}
