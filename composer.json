{
  "name": "kuaukutsu/ps-onion",
  "description": "Project Structure: onion",
  "license": "MIT",
  "type": "library",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Dmitriy Krivopalov",
      "email": "dimarik82@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/kuaukutsu/ps-onion/issues",
    "source": "https://github.com/kuaukutsu/ps-onion"
  },
  "require": {
    "php": ">=8.3",
    "ext-pdo": "*",
    "ext-sqlite3": "*",
    "guzzlehttp/guzzle": "^7.1",
    "monolog/monolog": "^3.7",
    "php-di/php-di": "^7.0.5",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^2.0",
    "psr/log": "^3.0",
    "psr/simple-cache": "^3.0",
    "ramsey/uuid": "^4.7",
    "type-lang/mapper": "^0.1"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.1",
    "phpstan/phpstan": "^1.12",
    "phpunit/phpunit": "^10.1",
    "rector/rector": "^1.2",
    "roave/infection-static-analysis-plugin": "^1.3",
    "roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "^8.7",
    "squizlabs/php_codesniffer": "^3.7",
    "vimeo/psalm": "^5.25"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "kuaukutsu\\ps\\onion\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kuaukutsu\\ps\\onion\\tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true
    }
  }
}
